  \def\bxce@parse@first@surrogate#1\bxce@parse@first@a#2{%
    \bxce@cnta=\numexpr\bxce@cnta*"400+`#2-"35FDC00\relax
    #1\bxce@parse@first@a % continue!
  }
\else
  \let\bxce@parse@first@trick\@gobble
\fi\fi

